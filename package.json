{
  "name": "higher-order-fetch",
  "version": "0.0.6",
  "description": "Using fetch with composable logic",
  "repository": "https://github.com/rudyhuynh/higher-order-fetch",
  "author": "rudyhuynh",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "pretest": "npm run build",
    "eslint": "eslint ./src",
    "doc": "documentation build src/** -f html -o docs",
    "public-doc": "npm run doc && node gh-pages.js"
  },
  "jest": {
    "setupFiles": [
      "./jest/globals.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.*"
    ],
    "collectCoverage": true
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-jest": "^22.7.2",
    "gh-pages": "^2.0.1",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0"
  },
  "peerDependencies": {
    "rxjs": "^6.5.2"
  }
}
